<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AlignTextView">
        <attr name="leading" format="dimension" />
        <attr name="firstLineLeading" format="dimension" />
        <attr name="lastLineDescent" format="dimension" />
    </declare-styleable>

    <declare-styleable name="StatusBarTextView">
        <attr name="textSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="OnyxToolbar">
        <attr name="weight" format="float"/>
    </declare-styleable>

    <declare-styleable name="CompoundButton">
        <attr name="android:button" />
    </declare-styleable>

    <declare-styleable name="CustomIconView">
        <attr name="iconDrawingSize" format="dimension" />
        <attr name="enableZoomDrawable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="pieChartView">
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension" />
        <attr name="progressTextColor" format="color" />
        <attr name="progressTextSize" format="dimension" />
        <attr name="max" format="float" />
        <attr name="textIsDisplayable" format="boolean" />
        <attr name="style">
            <enum name="STROKE" value="0"/>
            <enum name="FILL" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="AppCompatLayout">
        <attr name="alwaysLayoutImageView" format="boolean" />
    </declare-styleable>

    <declare-styleable name="DashedLine">
        <attr name="dashLine_orientation">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
    </declare-styleable>
</resources>