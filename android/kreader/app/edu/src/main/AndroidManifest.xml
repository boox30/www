        <activity android:name="com.onyx.android.edu.result.ResultActivity"></activity>
        <activity
            android:name="com.onyx.android.edu.teststype.TestsTypeActivity">
        </activity>
        <activity android:name="com.onyx.android.edu.chapter.ChapterTypeActivity"></activity>
        <activity
            android:name="com.onyx.android.edu.wrongquestions.WrongQuestionActivity">
        </activity>
        <activity android:name="com.onyx.android.edu.mytest.MyTestActivity">
        </activity>
        <activity android:name="com.onyx.android.edu.speakingtest.SpeakingTestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>