<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:bind="http://schemas.android.com/apk/res-auto">

    <data class="TilteSubjectItemBinding">

        <variable
            name="modulesBean"
            type="com.onyx.jdread.shop.cloud.entity.jdbean.BookModelConfigResultBean.DataBean.ModulesBean" />

    </data>

    <com.onyx.android.sdk.ui.compat.AppCompatLinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawablePadding="@dimen/view_all_drawable_padding"
            android:drawableRight="@mipmap/ic_go_view_all"
            android:ellipsize="end"
            android:gravity="center"
            android:maxLines="2"
            android:textColor="@android:color/black"
            android:text="@{modulesBean.show_name}"
            android:textSize="@dimen/level_two_heading_font" />

    </com.onyx.android.sdk.ui.compat.AppCompatLinearLayout>

</layout>