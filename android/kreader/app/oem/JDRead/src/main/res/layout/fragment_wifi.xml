<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data class="WifiBinding">

    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <include
            android:id="@+id/wifi_title_bar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            layout="@layout/setting_title_bar"/>

        <com.onyx.android.sdk.ui.view.PageRecyclerView
            android:paddingLeft="@dimen/fragment_content_padding"
            android:paddingRight="@dimen/fragment_content_padding"
            android:id="@+id/wifi_recycler"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </LinearLayout>
</layout>
